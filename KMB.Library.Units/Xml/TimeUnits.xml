<?xml version="1.0" encoding="utf-8" ?>
<units name="Time" ns="TimeUnits">
  <!-- Seconds already included in Metric -->
  <unit name="Minutes" shortname="min" dimension="Time" factor="60.0" />
  <unit name="Hours" shortname="hr" dimension="Time" factor="3600.0" />
  <unit name="Days" shortname="day" dimension="Time" factor="3600.0*24.0" />
  <unit name="Weeks" shortname="week" dimension="Time" factor="3600.0*24.0*7.0" />
  <unit name="JulianYears" shortname="yr" dimension="Time" factor="3600.0*24.0*365.25" />
  <unit name="SiderialYears" shortname="aₛ" dimension="Time" factor="3600.0*24.0*365.256363004" dontDisplay="true" />
</units>